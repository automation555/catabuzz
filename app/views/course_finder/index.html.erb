<h2>Search</h2>
<br />


<% form_tag('', :method=>:get) do -%>
<%= text_field_tag('q', '', :size => 30) %>
<%= submit_tag 'Search', :name=> nil %>
<% end -%>

<table border="0">
	<tr>
		<td /> <td />
	</tr>
<% unless @results.nil? %>
	<% for result in @results %>
		<% if result.is_a? CourseLecture%>
		<%= results.course.name%>
		<% end %>
	<% end %>

<% end %>

</table>

